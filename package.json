{
  "name": "beaver-dam",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0 --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "emu": "powershell -NoProfile -Command \"if (Test-Path scripts/emu.local.ps1) { powershell -ExecutionPolicy Bypass -File scripts/emu.local.ps1 } else { firebase emulators:start --import ./.emulator-data --export-on-exit }\"",
    "dev:emu": "concurrently -k -n EMU,WEB \"npm run emu\" \"npm run dev\""
  },
  "dependencies": {
    "firebase": "^10.14.1",
    "next": "^14.2.35",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "20.11.19",
    "@types/react": "18.2.46",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.20",
    "concurrently": "^9.2.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.35",
    "postcss": "8.4.47",
    "tailwindcss": "3.4.14",
    "typescript": "5.6.3"
  },
  "overrides": {
    "glob": "^10.4.6",
    "undici": "^6.21.2"
  }
}
