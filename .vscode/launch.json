{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Next.js: debug dev server",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "autoAttachChildProcesses": true
    }
  ]
}
